{
  "name": "shenzhiyong",
  "version": "1.0.6",
  "description": "A UI framework",
  "keywords": [
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest --watch",
    "test:nowatch": "cross-env CI=true jest",
    "dev": "npm run start",
    "clean": "rimraf ./build && rimraf ./dist",
    "lint": "eslint --ext tsx src --max-warnings 5",
    "build-ts": "tsc minify -p tsconfig.build.json",
    "build-css": "sass --style compressed ./src/styles/index.scss ./dist/style.css",
    "build-npm": "npm run clean && npm run build-ts && npm run build-css",
    "start": "webpack-dev-server --mode=development --config webpack.config.js",
    "build": "npm run clean && webpack --mode=production --config webpack.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublish": "npm run build-npm && npm config set registry https://registry.npmjs.org",
    "build:story": "yarn build-storybook -o ./path/to/build",
    "server:story": "npx http-server ./path/to/build"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/iFireflyTeam/iwhalecloud",
  "repository": {
    "type": "git",
    "url": "https://github.com/iFireflyTeam/iwhalecloud"
  },
  "author": "iFireflyTeam",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-flow": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.21",
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "antd": "^3.26.18",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "less": "^3.11.3",
    "less-loader": "^6.1.3",
    "mini-css-extract-plugin": "^0.11.2",
    "node-less": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "transform-class-properties": "^1.0.0-beta",
    "ts-loader": "^8.0.0",
    "tslint": "^6.1.2",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "antd": "^3.26.18"
  }
}
